{
  "name": "ng2-completer",
  "version": "0.2.3",
  "description": "ng2 autocomplete/typeahead component",
  "main": "bundles/ng2-completer.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "./node_modules/.bin/del dist .tmp bundles \"src/**/*.+(js|d.ts|js.map)\" \"demo/**/*.+(js|js.map)\" && mkdir dist",
    "prebuild": "npm run clean",
    "build": "npm run build:ngc && npm run bundle:umd && npm run packagejson && npm run copy",
    "build:ngc": "ngc -p tsconfig.bundle.json",
    "postbuild:ngc": "cp -R .tmp/esm/src/* dist",
    "copy": "npm run copy:assets && npm run copy:src",
    "copy:assets": "cp README.md dist",
    "copy:src": "cp -R src dist",
    "packagejson": "node ./create-package-json.js",
    "bundle:umd": "rollup -c rollup.config.js",
    "build:demo:dev": "NG2_COMPLETER_DEMO=y webpack --progress --color",
    "build:demo:prod": "NG2_COMPLETER_DEMO=y NODE_ENV=production webpack --progress --color",
    "start": "NG2_COMPLETER_DEMO=y webpack-dev-server --progress",
    "deploy:gh-pages": "npm run build:demo:prod && ./node_modules/.bin/gh-pages -d bundles"
  },
  "keywords": [
    "angular2",
    "autcomplete",
    "typeahead",
    "word completion",
    "ng2"
  ],
  "author": "Ofer Herman <oferhe@gmail.com>",
  "bugs": {
    "url": "https://github.com/oferh/ng2-completer/issues"
  },
  "license": "Apache-2.0",
  "typings": "./bundles/src/index.d.ts",
  "repository": "oferh/ng2-completer",
  "dependencies": {
    "@angular/common": "^2.1.2",
    "@angular/compiler": "^2.1.2",
    "@angular/core": "^2.1.2",
    "@angular/forms": "^2.1.2",
    "@angular/platform-browser": "^2.1.2",
    "@angular/platform-browser-dynamic": "^2.1.2",
    "rxjs": "5.0.0-beta.12"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.1.2",
    "@angular/http": "^2.1.2",
    "@angular/material": "^2.0.0-alpha.9-experimental-pizza",
    "@angular/platform-server": "^2.0.1",
    "@angular/router": "^3.0.0-rc.2",
    "@types/node": "^6.0.46",
    "@types/webpack": "^1.12.35",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^3.0.1",
    "core-js": "^2.4.1",
    "del-cli": "^0.2.0",
    "ejs": "^2.5.2",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "object.assign": "^4.0.3",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.3",
    "rollup": "^0.36.3",
    "rollup-plugin-uglify": "^1.0.1",
    "run-sequence": "^1.2.2",
    "ts-loader": "^0.8.1",
    "tslint": "^3.13.0",
    "typescript": "^2.0.9",
    "uglify-js": "^2.7.4",
    "unminified-webpack-plugin": "^1.1.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.10",
    "webpack-node-externals": "^1.5.4",
    "zone.js": "^0.6.26"
  }
}
